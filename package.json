    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "husky": "^0.14.3",
    "immer": "^8.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^7.2.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.12.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "4.35.3",
